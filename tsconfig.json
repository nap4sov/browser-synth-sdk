{
  "compilerOptions": {
    "incremental": true,
    "target": "ESNext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],    
    "module": "commonjs",      
    "declaration": true,    
    "outDir": "dist",
    "baseUrl": "./",            
    "esModuleInterop": true,      
    "skipLibCheck": true,          
    "forceConsistentCasingInFileNames": true 
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "test", "lib", "**/*spec.ts"]
}